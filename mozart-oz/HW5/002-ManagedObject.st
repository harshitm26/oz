Object subclass: #ManagedObject	instanceVariableNames: ''	classVariableNames: 'MaxInstances NInstances'	poolDictionaries: ''	category: 'ManagedObject'!"-- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- -- "!ManagedObject class	instanceVariableNames: ''!!ManagedObject class methodsFor: 'as yet unclassified'!initialize	super initialize.	MaxInstances := 4.	NInstances := 0! !!ManagedObject class methodsFor: 'as yet unclassified'!new	NInstances == MaxInstances		ifTrue: [self error: 'Cannot create more than 4 instances']		ifFalse: [NInstances := NInstances + 1.			super new]! !!ManagedObject class methodsFor: 'as yet unclassified'!ninstances	^ NInstances! !ManagedObject initialize!